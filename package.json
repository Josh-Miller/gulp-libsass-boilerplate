  "description": "A quick boilerplate for developing with gulp and libass",
  "scripts": {
    "test": "gulp test"
  },
  "dependencies": {
    "jquery": "1.11"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "browserify": "^6.3.2",
    "browserify-shim": "^3.8.1",
    "browsernizr": "^1.0.2",
    "event-stream": "^3.1.7",
    "glob": "^4.0.6",
    "grunt": "^0.4.5",
    "grunt-grunticon": "^2.1.2",
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-changed": "^1.0.0",
    "gulp-compass": "^2.0.1",
    "gulp-debug": "^1.0.1",
    "gulp-execsyncs": "^0.1.1",
    "gulp-filter": "^1.0.2",
    "gulp-livereload": "^2.1.1",
    "gulp-phpcs": "*",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^1.3.2",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "node-sass": "^2.0.0-beta",
    "nunjucks": "^1.1.0",
    "nunjucksify": "^0.1.2",
    "path": "^0.4.9",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "^1.0.0"
  },
  "browserify": {
